}\
 "jest": {
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/missing-types.d.ts",
      "!src/react-app-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 78,
        "branches": 58,
        "lines": 78,
        "functions": 65
      }
    }
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
